    "cac": "^6.7.14",
    "detect-browser": "^5.3.0",
    "fast-glob": "3.3.2",
    "fs-extra": "^11.2.0",
    "happy-dom": "^13.3.8",
    "hono-og": "0.0.16",
    "immer": "^10.0.3",
    "jose": "^5.2.2",
    "path-browserify": "^1.0.1",
    "qrcode": "^1.5.3",
    "viem": "^2.7.10",
    "vite": "^5.1.3"
  }
