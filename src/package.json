{
  "name": "frog",
  "version": "0.2.0",
  "type": "module",
  "module": "_lib/index.js",
  "types": "_lib/index.d.ts",
  "typings": "_lib/index.d.ts",
  "bin": {
    "frog": "./_lib/cli/index.js"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./_lib/index.d.ts",
      "edge": "./_lib/edge/index.js",
